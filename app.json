{
  "expo": {
    "name": "Muncie Events",
    "description": "Muncie Events Mobile App",
    "slug": "MuncieEvents",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "3.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
