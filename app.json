      "**/*"
    ],
    "ios": {

    },
    "android": {
      "versionCode": 3,
      "package": "com.muncieevents.MuncieEvents",
      "permissions": [
        "CAMERA",
        "MANAGE_DOCUMENTS"
      ]
    }
  }
}
